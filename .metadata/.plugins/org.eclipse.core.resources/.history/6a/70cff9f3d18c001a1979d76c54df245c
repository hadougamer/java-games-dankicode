package exemplo_oo;

public class Game {
	private Player player;
	private Inimigo inimigo;
	
	public Game() {
		player = new Player();
		inimigo = new Inimigo();
	}
	
	public Player getPlayer( ) {
		return this.player;
	}
	
	public Inimigo getInimigo( ) {
		return this.inimigo;
	}
	
	public static void main( String[] args ) {
		Game game = new Game();
	}
}
