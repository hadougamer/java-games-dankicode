package oo;

public class Main{
	private String name = "Alexandre";
	public static int final VIDA_MAXIMA = 100;
	
	public Main( String name ) {
		this.name = name;
		System.out.println("[constructor] Class Criada");
		System.out.println( this.name);
	}
	
	public static void main(String[] args) {
		new Main( "Alex" );
	}
}