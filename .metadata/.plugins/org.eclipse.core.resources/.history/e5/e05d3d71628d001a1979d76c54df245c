package inicio_logica_games;

public class Game implements Runnable {
	public boolean isRunning;
	
	public static void main ( String[] args ) {
		
	}

	// Update every loop cycle
	public void tick() {
	}
	
	// Display things
	public void render() {
	}
	
	@Override
	public void run() {
		while( isRunning ) {
			tick();
			render();
		}
	}
}
